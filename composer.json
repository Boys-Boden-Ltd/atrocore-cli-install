{
    "name": "atrocore/cli-install",
    "require": {
        "atrocore/core": ">=2.1.25 <2.3.0"
    },
    "conflict": {
        "atrocore/pim": "<1.14.0"
    },
    "autoload": {
        "psr-4": {
            "CliInstall\\": "app/"
        }
    },
    "extra": {
        "atroId": "CliInstall",
        "name": {
            "default": "CliInstall"
        },
        "description": {
            "default": "A custom module which provides a CLI tool for installing the application without the web installer."
        }
    }
}
